{
    "name": "pjhile/daterangepicker",
    "description": "A simple Date Range filter for Laravel Nova 4/5 that respects Nova sorting. Forked from the original rpj/daterangepicker package.",
    "keywords": [
        "laravel",
        "nova",
        "date",
        "daterange",
        "filter"
    ],
    "authors": [
        {
            "name": "PJ Hile"
        }
    ],
    "license": "MIT",
    "require": {
        "php": ">=8.1",
        "laravel/nova": "^4.0 | ^5.0"
    },
    "autoload": {
        "psr-4": {
            "Pjhile\\Daterangepicker\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Pjhile\\Daterangepicker\\FilterServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
